<h1>Posts</h1>
<table class="table table-hover">
	<% @posts.each do |post| %>
	<tr>
		<td><%= link_to post.title, admin_post_path(post) %><%=  %></td>
		<td class="del"><%= link_to '삭제', admin_post_path(post), :confirm => '정말로 삭제하시겠습니까?',:method => :delete %></td>
	</tr>
	<% end %>
</table>
<%= link_to "새 포스트",  new_admin_post_path, :class => 'btn btn-primary' %>
