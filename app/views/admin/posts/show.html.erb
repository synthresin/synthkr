<h1>Posts</h1>
<h3><%= @post.title %></h3>
<p><%= @post.content %></p>
<%= link_to "목록으로" , admin_posts_path, :class =>'btn' %>
<%= link_to "글 수정" , edit_admin_post_path(@post), :class =>'btn' %>
<%= link_to "글 삭제", admin_post_path(@post), :class => 'btn btn-danger', :method =>'delete', :confirm => '정말루 삭제하시겠습니까?' %>