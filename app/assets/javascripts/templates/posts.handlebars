		<div class="tools">
			{{#linkTo posts.new }}으앙{{/linkTo}}
		</div>
		<ul class="post-list"><!-- 이거 오버-->
			{{#each controller}}
				{{#unless isNew}}
				<li>
					{{#linkTo post this }}
						<h3>{{ title }}</h3>
						<p class="date">2013/01/13</p>
						<div class="arrow"></div>
					{{/linkTo}}
				</li>
				{{/unless}}
			{{/each}}
		</ul>
		{{outlet}}